#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../../../envify/3.4.1/envify/bin/envify" "$@"
  ret=$?
else 
  node  "$basedir/../../../../../envify/3.4.1/envify/bin/envify" "$@"
  ret=$?
fi
exit $ret
